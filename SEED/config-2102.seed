### INITIALIZATION ###
CONFIG_TARGET_rockchip=y
CONFIG_TARGET_rockchip_armv8=y
CONFIG_TARGET_MULTI_PROFILE=y
CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_friendlyarm_nanopi-r2s=y
CONFIG_TARGET_DEVICE_PACKAGES_rockchip_armv8_DEVICE_friendlyarm_nanopi-r2s="luci-app-frpc luci-i18n-frpc-zh-cn luci-app-oled luci-i18n-oled-zh-cn"
CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_friendlyarm_nanopi-r4s=y
CONFIG_TARGET_DEVICE_PACKAGES_rockchip_armv8_DEVICE_friendlyarm_nanopi-r4s="luci-app-frps luci-i18n-frps-zh-cn"
CONFIG_TARGET_PER_DEVICE_ROOTFS=y
CONFIG_TARGET_KERNEL_PARTSIZE=32
CONFIG_TARGET_ROOTFS_PARTSIZE=960
# CONFIG_TARGET_ROOTFS_SQUASHFS is not set

### AGGRESSIVE ###
CONFIG_DEVEL=y
CONFIG_TOOLCHAINOPTS=y
CONFIG_GCC_USE_VERSION_8=y
CONFIG_NODEJS_16=y
CONFIG_NODEJS_ICU_NONE=y

### BASIC ###
CONFIG_PACKAGE_autocore-arm=y
CONFIG_PACKAGE_coremark=y
CONFIG_PACKAGE_default-settings-chn=y
CONFIG_PACKAGE_ethtool=y
CONFIG_PACKAGE_htop=y
CONFIG_HTOP_LMSENSORS=y
CONFIG_PACKAGE_iftop=y
CONFIG_PACKAGE_ip6tables-legacy=y
CONFIG_PACKAGE_ip6tables-mod-fullconenat=y
